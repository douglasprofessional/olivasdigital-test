{
  "name": "olivasdigital/theme",
  "description": "Olivas Digital Test",
  "authors": [
    {
      "name": "Douglas Oliveira",
      "email": "perfildoug@gmail.com",
      "homepage": "https://douglasoliveira.dev.br"
    }
  ],
  "type": "wordpress-theme",
  "keywords": [
    "wordpress",
    "theme"
  ],
  "license": "GPL-2.0+",
  "autoload": {
    "psr-4": {
      "OD\\Theme\\": "lib/"
    }
  },
  "require": {
    "php": ">=5.6.0",
    "composer/installers": "~1.0",
    "behat/mink-extension": "^2.3",
    "behat/mink-goutte-driver": "^1.2"
  },
  "require-dev": {
    "bjornjohansen/wp-pre-commit-hook": "*",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "config": {
    "platform-check": false,
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
